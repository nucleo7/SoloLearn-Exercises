<html>
    <head></head>
    <body>
        <canvas id="canvas1" width="400" height="300">
        </canvas>
        <canvas id="canvas2" widtg="400" height="300">
        </canvas>
    <script>
        // Start
        var c1 = document.getElementById("canvas1");
        var ctx1 = c1.getContext("2d");
        c1.style.backgroundColor = "cyan";
        ctx1.font = "bold 22px Tahoma";
        ctx1.textAlign = "start";
        ctx1.fillStyle = "red";
        ctx1.fillText("start", 10, 30);
        
        // Not Start
        var c2 = document.getElementById("canvas2");
        var ctx2 = c2.getContext("2d");
        c2.style.backgroundColor = "magenta";
        ctx2.font = "bold 22px Arial";
        ctx2.textAlign = "Not start";
        ctx2.fillStyle = "green";
        ctx2.fillText("Not start", 100, 100);
    </script>
    </body>
</html>
