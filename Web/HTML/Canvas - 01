    <body>
        <canvas id="canvas1" width="400" height="150">
        </canvas>
        <canvas id="canvas2" width="400" height="150">
        </canvas>
        <canvas id ="canvas3" width="400" height="150">
    <script>
        var c = document.getElementById("canvas1");
        var ctx1 = c.getContext("2d");
        ctx1.fillStyle = "#FF0000";
        ctx1.fillRect(20,20,100,100);
        
        var d = document.getElementById("canvas2");
        var ctx2 = d.getContext("2d");
        ctx2.fillStyle = "#00FF00";
        ctx2.fillRect(250, 50, 120, 40);
        
        var e = document.getElementById("canvas3");
        var ctx3 = e.getContext("2d");
        ctx3.fillStyle = "#0000FF";
        ctx3.fillRect(0, 0, 100, 30);
    </script>
    </body>
