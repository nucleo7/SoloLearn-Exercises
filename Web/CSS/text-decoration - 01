/* Exercises from Course Material */
b.none {
    text-decoration: none;
}
.inherit {
    text-decoration: inherit;
}
b.overline {
    text-decoration: overline;
}
b.underline {
    text-decoration: underline;
}
b.line-through {
    text-decoration: line-through;
}
