/* rotateX(300deg) rotateY(250deg) rotateZ(70deg) */
div.four {
    background-color: red;
    border: 1px solid black;
    color: yellow;
    float: none;
    font-size: 0.5em;
    font-weight: bold;
    height: 70px;
    width: 250px;
