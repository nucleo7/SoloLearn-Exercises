/* Exercises from Course Material */

/* Red bar */
div#One {
    animation-duration: 5s;
    animating-name: cChange1;
    background-color: red;
    color: white;
    font-weight: bold;
    height: 70px;
    width: 200px;
    -webkit-animation-duration: 5s;
    -webkit-animation-name: cChange1;
}
@keyframes cChange1 {
    from { width: 0px; }
    to { width: 200px; }
}
@-webkit-keyframes cChange1 {
    from { width: 0px; }
    to { width: 200px; }
}
