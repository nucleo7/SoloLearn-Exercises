/* Exercises from Course Material */

/* Note: the order of the short-hand animation properties matter! */
div#one {
    animation: change1 3s ease-in 1s infinite reverse;
    background-color: red;
    color: black;
    font-weight: bold;
    height: 70px;
    width: 250px;
    -webkit-animation: change1 3s ease-in 1s infinite reverse;
}
@keyframes change1 {
    from { width: 0px; }
    to { width: 250px; }
}
@-webkit-keyframes change1 {
    from { width: 0px; }
    to { width: 250px; }
}
div#two {
    animation: change2 3s ease-out 3;
    background-color: green;
    color: yellow;
    font-weight: bold;
    height: 70px;
    width: 250px;
    -webkit-animation: change2 3s ease-out 3;
}
@keyframes change2 {
    from { width: 0px; }
    to { width: 250px; }
}
@-webkit-keyframes change2 {
    from { width: 0px; }
    to {width: 250px; }
}
div#three {
    animation: change3 3s cubic-bezier(0, 0.2, 0.8, 1) 3 alternate;
    background-color: blue;
    color: magenta;
    font-weight: bold;
    height: 70px;
    width: 250px;
    -webkit-animation: change3 3s cubic-bezier(0, 0.2, 0.8, 1) 3 alternate;
}
@keyframes change3 {
    from { width: 0px; }
    to { width: 250px; }
}
@-webkit-keyframes change3 {
    from { width: 0px; }
    to { width: 250px; }
}
/* four - animate all the things */
div#four {
    animation: change4 3s linear 3 alternate;
    background-color: red;
    color: cyan;
    font-weight: bold;
    height: 250px;
    width: 250px;
    -webkit-animation: change4 3s linear 3 alternate;
}
@keyframes change4 {
    from { height: 0px;
           width: 250px;
           background-color: red;
           color: cyan; }
    to { height: 250px;
         width: 0px;
         background-color: blue;
         color: yellow; }
}
@-webkit-keyframes change4 {
    from { height: 0px;
           width: 250px;
           background-color: red;
           color: cyan; }
    to { height: 250px;
         width: 0px;
         background-color: blue;
         color: yellow; }
}
