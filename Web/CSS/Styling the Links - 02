/* Exercises from Course Material */

a:link {
    text-decoration: none;
    color: cyan;
    background-color: red;
}

a:visited {
    text-decoration: none;
    color: magenta;
}

a:hover {
    text-decoration: none;
    color: yellow;
    background-color: green;
}

a:active {
    text-decoration: none;
    color: black;
    background-color: blue;
}
