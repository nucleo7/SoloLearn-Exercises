// Exercises from Course Material
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = 10;
            int y = 7;
            int z = 33;
            Console.WriteLine(x);
            Console.WriteLine(y);
            Console.WriteLine(z);
            x++;
            y++;
            z++;
            Console.WriteLine(x);
            Console.WriteLine(y);
            Console.WriteLine(z);
            // EDIT: Added these later
            Console.WriteLine(++x);
            Console.WriteLine(++y);
            Console.WriteLine(++z);
            Console.WriteLine(x++);
            Console.WriteLine(y++);
            Console.WriteLine(z++);
        }
    }
}
