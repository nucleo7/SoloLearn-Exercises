// Exercises from Course Material
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            
            for (int x = 3; x < 11; x++) {
               if (x == 7)
                   continue;
                Console.WriteLine("The value of x is: " + x);
            }
            
            for (int y = 3; y <= 11; y++) {
               if (y == 7)
                   continue;
                Console.WriteLine("The value of y is: " + y);
            }
        }
    }
}
