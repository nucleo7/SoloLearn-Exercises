// Exercises from Course Material
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Print(int a) {
            Console.WriteLine("Value: " + a);
        }
        static void Print(double a) {
            Console.WriteLine("Value: " + a);
        }
        static void Print(string label, double a) {
            Console.WriteLine(label + a);
        }
        
        static void Something(int x) {
            Console.WriteLine("The printed value is: " + x);
        }
        static void Something(double x) {
            Console.WriteLine("The printed value is: " + x);
        }
        static void Something(string x) {
            Console.WriteLine("The printed value is: " + x);
        }
        static void Something(int x, double y, string z) {
            Console.WriteLine("The value of x is: " + x + ", and the value of y is: " + y + ", and the value of z is: " + z);
        }
        
        static void Main(string[] args)
        {
            Print(11);
            Print(4.13);
            Print("Average: ", 7.57);
            
            Something(7);
            Something(1.34);
            Something("Some thing!");
            Something(3, 7.13, "Thing!");
        }
    }
}
