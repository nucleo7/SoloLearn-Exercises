// Exercises from Course Material
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static int Multiply(int a, int b)
        {
            return a * b;
        }
        static int Divide(int c, int d)
        {
            return c / d;
        }
        static int Sum(int e, int f)
        {
            return e + f;
        }
        static int Subtract(int g, int h)
        {
            return g - h;
        }
        static int Remainder(int i, int j)
        {
            return i % j;
        }
        
        static void Main(string[] args)
        {
            Console.WriteLine(Multiply(1, 3));
            Console.WriteLine(Divide(5, 7));
            Console.WriteLine(Sum(11, 13));
            Console.WriteLine(Subtract(17, 23));
            Console.WriteLine(Remainder(29, 31));
        }
    }
}
