// Exercises from Course Material

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int[ ] arrOne = {2, 4, 7};
            Console.WriteLine(arrOne.Length);
            Console.WriteLine(arrOne.Rank);
            
            
            int[] arrayTwo = {1, 3, 5, 7};
            int[] arrayThree = {11, 13};
            
            Console.WriteLine("arrayTwo length: " + arrayTwo.Length);
            Console.WriteLine("arrayThree length: " + arrayThree.Length);
            
            Console.WriteLine("arrayTwo rank: " + arrayTwo.Rank);
            Console.WriteLine("arrayThree rank: " + arrayThree.Rank);
            
            
            int[][] arrayFour = {new int[] {3, 5}, new int[] {7, 11, 13}};
            int[][] arrayFive = {new int[] {1, 2}, new int[] {3, 4}, new int[] {5, 6}};
            
            Console.WriteLine("arrayFour length: " + arrayFour.Length);
            Console.WriteLine("arrayFive length: " + arrayFive.Length);
            
            Console.WriteLine("arrayFour rank: " + arrayFour.Rank);
            Console.WriteLine("arrayFive rank: " + arrayFive.Rank);
        }
    }
}
