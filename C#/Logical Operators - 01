namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            
            int numOne = 7;
            int numTwo = 3;
            
            if (numOne > 0 && numTwo > 0) {
               Console.WriteLine("The numOne value is: " + numOne + " | " + "The numTwo value is: " + numTwo);
            }
        }
    }
}
