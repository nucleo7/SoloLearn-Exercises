// Exercises from Course Material 

  

// Animal superclass 

class Animal { 

    public void makeSound() { 

        System.out.println("Grr..."); 

    } 

} 

  

// Cat subclass of Animal class 

class Cat extends Animal { 

    public void makeSound() { 

        System.out.println("Meow"); 

    } 

} 

  

// Dog subclass of Animal class 

class Dog extends Animal { 

    public void makeSound() { 

        System.out.println("Woof"); 

    } 

} 

  

  

// SuperClass superclass 

class SuperClass { 

    public void theOutput() { 

        System.out.println("This is the SuperClass."); 

    } 

} 

  

// subclass 1 of superclass 

class SubClassOne extends SuperClass { 

    public void theOutput() { 

        System.out.println("This is SubClassOne."); 

    } 

} 

  

// subclass 2 of superclass 

class SubClassTwo extends SuperClass { 

    public void theOutput() { 

        System.out.println("This is SubClassTwo."); 

    } 

} 

  

class Program { 

    public static void main(String args[ ]) { 

        // Creating and calling Animal 

        Animal a1 = new Animal(); 

        Animal d1 = new Dog(); 

        Animal c1 = new Cat(); 

         

        a1.makeSound(); 

        d1.makeSound(); 

        c1.makeSound(); 

         

        // Creating and calling SuperClass 

        SuperClass s1 = new SuperClass(); 

        SuperClass s2 = new SubClassOne(); 

        SuperClass s3 = new SubClassTwo(); 

         

        s1.theOutput(); 

        s2.theOutput(); 

        s3.theOutput(); 

        s2.theOutput(); 

        s1.theOutput(); 

        s3.theOutput(); 

    } 

} 
