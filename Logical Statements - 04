public class Program { 

    public static void main(String[] args) { 

        int age = 25; 

        int year = 1980; 

         

        if(!(age < 18) && !(year > 1985)) { 

            System.out.println("Too Young"); 

        } else { 

            System.out.println("Welcome"); 

        } 

    } 

} 
