// Exercises from Course Material 

public class Program { 

    public static void main(String[] args) { 

        int x = 5; 

        System.out.println(x); 

        ++x; 

        System.out.println(x); 

         

        int y = 63; 

        System.out.println(y); 

        ++y; 

        System.out.println(y); 

         

        System.out.println(x + y); 

        System.out.println(++x + ++y); 

    } 

} 
