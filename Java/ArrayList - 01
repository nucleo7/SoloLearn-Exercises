// Exercises from Course Material

// ArrayList needs to be imported for strings
// List needs to be imported for integers
import java.util.ArrayList;
import java.util.List;

public class MyClass {
    public static void main(String[ ] args) {
        // colorsRGB ArrayList
        ArrayList<String> colorsRGB = new ArrayList<String>();
        colorsRGB.add("Red");
        colorsRGB.add("Green");
        colorsRGB.add("Blue");
        
        System.out.println("colorsRGB = " + colorsRGB);
        
        // colorsCMYK ArrayList
        ArrayList<String> colorsCMYK = new ArrayList<String>();
        colorsCMYK.add("Cyan");
        colorsCMYK.add("Magenta");
        colorsCMYK.add("Yellow");
        colorsCMYK.add("blacK");
        
        System.out.println("colorsCMYK = " + colorsCMYK);
        
        // shapes ArrayList
        ArrayList<String> shapes = new ArrayList<String>();
        shapes.add("circle");
        shapes.add("line");
        shapes.add("polygon");
        shapes.add("rectangle");
        shapes.add("square");
        shapes.add("triangle");
        
        System.out.println("shapes = " + shapes);
        
        // numbers ArrayList, not integers!
        ArrayList<String> numbersOne = new ArrayList<String>();
        numbersOne.add("1");
        numbersOne.add("2");
        numbersOne.add("3");
        
        System.out.println("numbersOne = " + numbersOne);
        
        // numbers ArrayList/List, with integers!
        List<Integer> numbersTwo = new ArrayList<>();
        numbersTwo.add(1);
        numbersTwo.add(2);
        numbersTwo.add(3);
        
        System.out.println("numbersTwo = " + numbersTwo);
    }
}
