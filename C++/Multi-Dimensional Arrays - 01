// Exercises from Course Material
#include <iostream>
using namespace std;

int main()
{
    int x[2][3] = {{2, 3, 4}, {8, 9, 10}};
    cout << "x: " << x[0][2] << endl;
    
    
    int y[2][3] = {
    {2, 3, 4}, // 1st row
    {8, 9, 10} // 2nd row
    };
    cout << "y: " << y[1][1] << endl;
    
    int a[1][3] = {1, 3, 5};
    int b[1][3] = {
        1, 3, 5
    };
    cout << "a: " << a[0][1] << endl;
    cout << "b: " << b[0][2] << endl;
    
    int c[3][1] = {{7}, {11}, {13}};
    int d[3][1] = {
        {7},
        {11},
        {13}
    };
    cout << "c: " << c[2][0] << endl;
    cout << "d: " << d[1][0] << endl;
    
    int e[3][5] = {{1, 3, 5, 7, 11}, {13, 15, 17, 19, 23}, {29, 31, 37, 41, 43}};
    int f[3][5] = {
        {1, 3, 5, 7, 11},
        {13, 15, 17, 19, 23},
        {29, 31, 37, 41, 43}
    };
    cout << "e: " << e[2][1] << endl;
    cout << "f: " << f[1][2] << endl;
    
    return 0;
}
