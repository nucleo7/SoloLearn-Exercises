// Exercises from Course Material

#include <iostream>
using namespace std;

int volume(int l=1, int w=1, int h=1) {
    return l*w*h;
}

int area(int l1 = 3, int w1 = 5) {
    return l1 * w1;
}

int main() {
    cout << "volume: " << volume() << endl;    // 1 * 1 * 1
    cout << "volume: " << volume(5) << endl;    // 5 * 1 * 1
    cout << "volume: " << volume(2, 3) << endl;    // 2 * 3 * 1
    cout << "volume: " << volume(3, 7, 6) << endl;    // 3 * 7 * 6
    
    cout << "area: " << area() << endl;    // 3 * 5
    cout << "area: " << area(3) << endl;    // 3 * 5
    cout << "area: " << area(5) << endl;    // 5 * 5
    cout << "area: " << area(3, 5) << endl;    // 3 * 5
}
