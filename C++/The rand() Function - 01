// Exercises from Course Material

#include <iostream>
#include <cstdlib>
using namespace std;

int main() {
  cout << "Just random: " << rand() << endl;
  
  float x = rand() * 0.5;
  float y = rand() % 10;
  
  cout << "Random x: " << x << endl;
  cout << "Random y: " << y << endl;
}
