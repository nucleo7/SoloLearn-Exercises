// Exercises from Course Material
#include <iostream>
using namespace std;

int main() 
{
    int a, b, c;
    int sum;
    cout << "Enter number 1: \n";
    cin >> a;
    cout << a << endl;
    
    cout << "Enter number 2: \n";
    cin >> b;
    cout << b << endl;
    
    cout << "Enter number 3: \n";
    cin >> c;
    cout << c << endl;
    
    sum = a + b + c;
    cout << "Sum is: " << sum << endl;

  return 0;
}
