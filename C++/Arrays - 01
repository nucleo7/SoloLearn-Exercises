// Exercises from Course Material
#include <iostream>
using namespace std;

int main()
{
    int b[] = {11, 45, 62, 70, 88};

    cout << b[0] << endl;
    // Outputs 11
    cout<< b[3] << endl;
    // Outputs 70
    
    int x[4] = {1, 3, 5, 7};
    int y[] = {11, 13, 17, 19};
    
    cout << "The value of index 2 in array x is: " << x[2] << endl;
    cout << "The value of index 2 in array y is: " << y[2] << endl;
    
    return 0;
}
