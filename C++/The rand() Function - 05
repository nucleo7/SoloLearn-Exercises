// Exercises from Course Material

#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

// In order to get truly random numbers, the current time is used with srand to seed the rand function. When time(0) is used, the current time is used (in seconds)

int main () {
    srand(time(0));
    
    for (int x = 1; x <= 10; x++) {
        cout << "x: " << 1 + (rand() % 6) << endl;
    }
    
    for (int y = 5; y < 11; y++) {
        cout << "y: " << 5 + (rand() % 13) << endl;
    }
}
