// Exercises from Course Material

#include <iostream>
#include <cstdlib>
using namespace std;

int main() {
    for (int x = 1; x < 5; x++) {
        cout << "x: " << rand() << endl;
    }
    
    for (int y = 0; y <= 5; y++) {
        cout << "y: " << rand() << endl;
    }
}
