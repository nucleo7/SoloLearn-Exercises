// Exercises from Course Material

#include <iostream>
using namespace std;

int main()
{
    int *p = NULL; // Pointer initialized with null
    p = new int[20]; // Request memory
    delete [] p; // Delete array pointed to by p
    
    int *x = NULL;
    cout << "*x: " << *x << endl;
    cout << "x: " << x << endl;
    
    x = new int[42];
    cout << "*x: " << *x << endl;
    cout << "x: " << x << endl;
    
    delete [] x;
    cout << "*x: " << *x << endl;
    cout << "x: " << x << endl;
    
    return 0;
}
