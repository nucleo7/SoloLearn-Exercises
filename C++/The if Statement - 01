// Exercises from Course Material
#include <iostream>
using namespace std;

int main()
{
    if (7 > 4) {
        cout <<  "Yes, 7 is greater than 4." << endl; 
    }
    
    if (7 < 11) {
       cout << "Yes, 7 is less than 11." << endl;
    }
    
    return 0;
}
