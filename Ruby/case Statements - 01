# Exercises from Course Material
numOne = 3
case numOne
when 1
    puts "numOne value is 1."
when 2
    puts "numOne value is 2."
when 3
    puts "numOne value is 3."
end

numTwo = 13
case numTwo
when 11
    puts "numTwo value is 11."
when 12
    puts "numTwo value is 12."
when 14
    puts "numTwo value is 14."
end
