# Exercises from Course Material
arr = [2, 4, 6]
sum = 0
arr.each do |x|
    sum += x
end

puts sum

arrayOne = [1, 3, 5, 7]
print arrayOne
puts

sumOne = 0
arrayOne.each do |a|
    sumOne += a
end

puts "The sum of the elements of arrayOne is: #{sumOne}"
