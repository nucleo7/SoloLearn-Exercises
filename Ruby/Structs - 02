# Exercises from Course Material

require "ostruct"

# Person1
person1 = OpenStruct.new
person1.name = "John"
person1.age = 42
person1.salary = 250

puts "person1.name: #{person1.name}"
puts "person1.age: #{person1.age}"
puts "person1.salary: #{person1.salary}"

# Person2
person2 = OpenStruct.new
person2.name = "Jack"
person2.age = 27
person2.salary = 222

puts "person2.name: #{person2.name}"
puts "person2.age: #{person2.age}"
puts "person2.salary: #{person2.salary}"

# Vehicle1
vehicle1 = OpenStruct.new
vehicle1.brand = "Lamborghini"
vehicle1.model = "Aventador"
vehicle1.year = 2017
vehicle1.price = "a lot"
vehicle1.speed = "fast"

puts "vehicle1: #{vehicle1.brand} #{vehicle1.model} from #{vehicle1.year} is #{vehicle1.speed} and costs #{vehicle1.price}"
