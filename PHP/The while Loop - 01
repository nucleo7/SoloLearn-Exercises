<!-- Exercises from Course Material -->
<?php

    $numOne = 3;
    $numTwo = 11;
    echo "The starting value of numOne before the loop is: $numOne <br />";
    
    while ($numOne < $numTwo) {
        echo "The value of numOne is: $numOne <br />";
        $numOne++;
    }
    
    echo "The final value of numOne after the loop is: $numOne <br />";
    
?>
