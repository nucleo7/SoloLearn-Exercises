// Exercises from Course Material 

public class Program { 

    public static void main(String[] args) { 

        int x = 34; 

        System.out.println("x: " + x); 

         

        int y = ++x; 

        System.out.println("x: " + x + " | " + "y: " + y); 

        int z = ++x; 

        System.out.println("x: " + x + " | " + "y: " + y + " | " + "z: " + z); 

    } 

} 
